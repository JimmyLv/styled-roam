    "dateformat": "^4.5.1",
    "dayjs": "^1.9.8",
    "dropbox": "^9.6.0",
    "dropbox-dropins": "^1.0.4",
    "filepond": "^4.27.0",
    "gh-pages": "^3.1.0",
    "hotkeys-js": "^3.8.1",
